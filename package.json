{
  "name": "utree",
  "version": "0.2.1",
  "description": "A microscopically small universal self-balancing tree",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive test",
    "build": "browserify src/index.js -t [ babelify --no-sourceMaps ] > index.js",
    "build-debug": "browserify src/index.js -d -t [ babelify --no-sourceMaps ] > index.js",
    "watch": "watchify src/index.js -o index.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/download/utree.git"
  },
  "keywords": [
    "binary",
    "tree",
    "rebalance"
  ],
  "author": "JT Bowler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/download/utree/issues"
  },
  "homepage": "https://github.com/download/utree#readme",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "underscore": "^1.8.3",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
